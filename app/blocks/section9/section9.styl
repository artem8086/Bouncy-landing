.section9
	background-color #f3f3f3
	padding 80px 0 100px

	&__header
		padding-bottom 40px
		text-align center
		font-size 20px

	&__desc
		text-align center
		padding-bottom 60px

.sec9-slider
	background-color main-color
	padding 52px 0 60px

.sec9-page
	&__ava
		border-radius 50%
		width 130px
		height 130px

	&__desc
		color #81868e
		font-size 16px
		line-height 28px
		letter-spacing .8px
		padding-bottom 30px
	
	&__name
		color #19bd9a
		font-size 16px
		font-weight 700
		text-transform uppercase
		letter-spacing .64px
	
	&__role
		color #464646
		font-size 14px
		text-transform uppercase
		letter-spacing .56px
		